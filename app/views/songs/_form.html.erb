<%= form_for [@artist.songs.new] do |f| %>
<div class = "row">

  <div class="panel panel-warning col-md-6 col-md-offset-3">

      <div class="panel-heading text-center">
        <h3>Add New Song</h3>
      </div>

      <div class="panel-body text-center">
        <div class="form-group">
            <label>Song name</label>
             <%= f.text_field :song_name, placeholder: "Add song name", class: "form-control" %>
        </div>

        <div class="form-group">
            <label>Song URL</label>
             <%= f.text_field :song_url, placeholder: "Add song url", class: "form-control" %>
        </div>

        <div class="actions">
         <%= f.submit "Save", class: "btn btn-success" %>
        </div>

        <% end %>
      </div>

    </div>

  </div>
